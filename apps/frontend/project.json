{
  "name": "frontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/frontend",
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run dev",
        "cwd": "apps/frontend"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run build",
        "cwd": "apps/frontend"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun run preview",
        "cwd": "apps/frontend"
      }
    }
  },
  "tags": ["ui", "frontend"]
}
